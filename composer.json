{
    "name": "intellitask/intellitask",
    "description": "A Laravel project for task management.",
    "type": "project",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "laravel/framework": "^10.0",
        "laravel/tinker": "^2.0",
        "livewire/livewire": "*"
    },
    "require-dev": {
        "laravel/breeze": "1.29",
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover --ansi"
        ],
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
